  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Projekat</DataSourceName>
        <CommandText>SELECT ranking.ranking_points, ranking.id_rank, ranking.player_id, player.firstName, player.lastName
FROM   ranking INNER JOIN
             player ON ranking.id_rank = player.id_rank</CommandText>
      </Query>
      <Fields>
        <Field Name="ranking_points">
          <DataField>ranking_points</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="id_rank">
          <DataField>id_rank</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="player_id">
          <DataField>player_id</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="firstName">
          <DataField>firstName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="lastName">
          <DataField>lastName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Filters>
        <Filter>
          <FilterExpression>=Fields!firstName.Value</FilterExpression>
          <Operator>Like</Operator>
          <FilterValues>
            <FilterValue> Ricardo</FilterValue>
          </FilterValues>
        </Filter>
      </Filters>
    </DataSet>
